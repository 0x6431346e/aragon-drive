  "scripts": {
    "start": "aragon run --kit Kit --kit-init @ARAGON_ENS",
    "start:frontend": "cd app/frontend/ && npm run start-story",
    "devchain": "aragon devchain",
    "publish:datastore-acl": "cd apps/datastore-acl && aragon apm publish --automatically-bump",
    "test": "aragon contracts test",
    "compile": "aragon contracts compile",
    "sync-assets": "mkdir -p dist; rsync -rtu \"$(dirname $(node -p 'require.resolve(\"@aragon/ui\")'))/\" dist/aragon-ui",
    "build:app": "parcel build app/index.html -d dist/ --no-minify --public-url '.'",
    "build:script": "parcel build app/script.js -d dist/",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "lint": "eslint app",
    "lint:fix": "eslint app --fix",
    "publish": "aragon publish",
    "preinstall": "npm install --prefix app/frontend && npm install --prefix apps/datastore-acl"
